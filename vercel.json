{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/socket"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npx prisma generate && npm run build"
}
